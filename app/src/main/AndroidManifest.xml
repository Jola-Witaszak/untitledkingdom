<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.untitledkingdom.weatherwoman">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/Theme.Weatherwoman">
        <activity
                android:name=".AllCitiesActivity"
                android:label="@string/title_activity_all_cities"
                android:theme="@style/Theme.Weatherwoman.NoActionBar">
        </activity>
        <activity
                android:name=".LowAvgTempActivity"
                android:label="@string/title_activity_low_avg_temp"
                android:theme="@style/Theme.Weatherwoman.NoActionBar">
        </activity>
        <activity
                android:name=".MaxTempActivity"
                android:label="@string/title_activity_max_temp"
                android:theme="@style/Theme.Weatherwoman.NoActionBar">
        </activity>
        <activity
                android:name=".MinTempActivity"
                android:label="@string/title_activity_min_temp"
                android:theme="@style/Theme.Weatherwoman.NoActionBar">
        </activity>
        <activity android:name=".WeatherForecastActivity">
        </activity>
        <activity android:name=".WeatherwomanActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>